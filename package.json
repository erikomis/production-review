    "nodemailer": "^6.7.5",
    "pg": "^8.7.3",
    "prisma": "^3.15.1",
    "regenerator-runtime": "^0.13.9",
    "tsyringe": "^4.7.0",
    "typescript": "^4.7.3"
  },
  "devDependencies": {
    "@swc-node/register": "^1.5.1",
